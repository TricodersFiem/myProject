                android:text="MCS 401" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="HU 491" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="match_parent"
